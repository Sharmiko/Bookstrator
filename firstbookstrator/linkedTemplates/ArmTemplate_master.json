{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "firstbookstrator"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureHDInsightLinkedService_clusterPassword": {
			"type": "secureString",
			"metadata": "Secure string for 'clusterPassword' of 'AzureHDInsightLinkedService'"
		},
		"AzureHDInsightLinkedService_clusterSshPassword": {
			"type": "secureString",
			"metadata": "Secure string for 'clusterSshPassword' of 'AzureHDInsightLinkedService'"
		},
		"AzureHDInsightLinkedService_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'AzureHDInsightLinkedService'"
		},
		"BookstratorLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BookstratorLinkedService'"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_hostSubscriptionId": {
			"type": "string",
			"defaultValue": "cd309673-f356-437c-b53a-ef46d5ec9635"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_clusterResourceGroup": {
			"type": "string",
			"defaultValue": "bookstrator_intro"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "67a953ab-954d-4d9d-99c2-e3d04c10344f"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "4e15d6e6-5843-454b-a458-ac5af13716de"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_clusterUserName": {
			"type": "string",
			"defaultValue": "ClusterName"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"BookstratorLinkedService_connectionString": {
						"value": "[parameters('BookstratorLinkedService_connectionString')]"
					},
					"AzureHDInsightLinkedService_clusterPassword": {
						"value": "[parameters('AzureHDInsightLinkedService_clusterPassword')]"
					},
					"AzureHDInsightLinkedService_clusterSshPassword": {
						"value": "[parameters('AzureHDInsightLinkedService_clusterSshPassword')]"
					},
					"AzureHDInsightLinkedService_servicePrincipalKey": {
						"value": "[parameters('AzureHDInsightLinkedService_servicePrincipalKey')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_hostSubscriptionId": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_hostSubscriptionId')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_clusterResourceGroup": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_clusterResourceGroup')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_tenant": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_tenant')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_clusterUserName": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_clusterUserName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}